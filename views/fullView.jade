    script(src='/assets/js/bootstrap.min.js')
    script(type="text/javascript",src="/assets/js/HTTPRequest.js")
    script(type="text/javascript"). 
      cameraId = '#{cameraId}'
      jobId = '#{jobId}'
      owner = '#{owner}'
    script(src='/assets/js/fullView.js')
